<div class="detail-box">
	<div class="row">
		<div class="col-xs-12">
			<h3>Members</h3>
		</div>
	</div>
	<div class="row">
		<% 
            if (native.members.length<=0) {
        %>
            <div class="col-xs-12">
                <h6>This native has no memeber yet</h6>
            </div>
        <% } else {
        for(var i=0; i<native.members.length; i++) {%>
            <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3">
                <div class="wrapper-card">
                            
                    <div class="card radius shadowDepth1">
                        <div class="card__image border-tlr-radius">
                            <img src="/img/team/pic-1.png">
                        </div>

                        <div class="card__content card__padding">
                            <div class="card__share">
                                <div class="card__social">  
                                    <a class="share-icon facebook" href="#"><span class="fa fa-facebook"></span></a>
                                    <a class="share-icon twitter" href="#"><span class="fa fa-twitter"></span></a>
                                    <a class="share-icon medium" href="#"><span class="fa fa-medium"></span></a>
                                </div>

                                <a id="share" class="share-toggle share-icon" href="#"></a>
                            </div>

                            <div class="card__meta">
                                <a href="#">Jake V.</a>
                                <!--<time>170.000 ETH</time>-->
                            </div>

                        </div>

                    </div>

                </div>
            </div>
        <% }} %>
	</div>
</div>