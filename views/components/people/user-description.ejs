<div class="detail-box">
	<div class="row">
		<div class="col-xs-12 col-sm-4 col-md-6 col-lg-6">
			<h1><%= user.fullName %> <% if(user.verified) { %><i class="verified fa fa-check" title="Verified user"></i><% } %></h1>
			<h6><%= user.primaryIntent %></h6>
			<h6><strong>Website: </strong><a href="<%= user.websiteUrl %>"><%= user.websiteUrl %></a></h6>
			
			<% if("undefined" !== typeof user.facebookUrl) { %>
	            <a href="" class="btn btn-height btn-social-icon btn-facebook"><span class="fa fa-facebook"</span></a>
	        <% } %>
	        <% if("undefined" !== typeof user.twitterUrl) { %>
	            <a href="" class="btn btn-height btn-social-icon btn-twitter"><span class="fa fa-twitter"</span></a>
	        <% } %>
	        <% if("undefined" !== typeof user.linkedinUrl) { %>
	            <a href="" class="btn btn-height btn-social-icon btn-linkedin"><span class="fa fa-linkedin"</span></a>
	        <% } %>

		</div>
		<div class="col-xs-12 col-sm-8 col-md-6 col-lg-6">
			<p><%= user.bio %></p>
		</div>
	</div>
</div>